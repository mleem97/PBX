# Production Environment Variables
# Copy this file to .env and adjust values as needed

# Container Image Settings
IMAGE_REGISTRY=ghcr.io
IMAGE_REPO=your-username/freepbx-docker
IMAGE_TAG=latest

# Port Configuration
HTTP_PORT=8080
HTTPS_PORT=8443
SIP_PORT=5060
SIPS_PORT=5061
RTP_START=10000
RTP_END=20000

# System Configuration
TIMEZONE=Europe/Berlin
MYSQL_ROOT_PASSWORD=

# SSL Configuration (for HTTPS)
SSL_CERT_PATH=./ssl

# Logging Configuration
LOG_LEVEL=INFO
LOG_MAX_SIZE=100m
LOG_MAX_FILES=5